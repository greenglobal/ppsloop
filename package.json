    "babel-plugin-external-helpers": "6.x.x",
    "babel-preset-env": "1.x.x",
    "babel-preset-es2015-rollup": "3.x.x",
    "bellajs": "6.x.x",
    "copy-dir": "0.x.x",
    "css-mqpacker": "6.x.x",
    "cssnano": "3.x.x",
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "html-minifier": "3.x.x",
