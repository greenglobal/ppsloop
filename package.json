{
  "version": "0.6.11",
  "name": "ppsw",
  "description": "ppsloop demo",
  "homepage": "http://beta.greenglobal.vn",
  "repository": {
    "type": "git",
    "url": "git@github.com:greenglobal/ppsloop.git"
  },
